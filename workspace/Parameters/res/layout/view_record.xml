          android:layout_height="150dp"
          android:background="#1E90FF" 
          android:layout_marginLeft="10dp"
          android:layout_marginRight="10dp"
          android:layout_marginTop="10dp" >
				<ImageView android:id="@+id/dissolved_oxygen_image"
			                 android:layout_width="75dp"
			                 android:layout_height="125dp"
			                 android:layout_marginLeft="10dp"
			                 android:layout_marginTop="0dp"
			                 android:src="@drawable/image_7"
			                    />          
                <TextView android:id="@+id/dissolved_oxygen_value"
	                android:layout_toRightOf="@id/dissolved_oxygen_image"
	                android:layout_marginLeft="40dp"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:textColor="#ffffff"
	                android:textSize="25dp"
	                android:text=""
	          />
	      
                <ImageView 
                    android:id="@+id/dissolved_oxygen_arrow"
                    android:layout_toRightOf="@id/dissolved_oxygen_image"
                    android:layout_below="@id/dissolved_oxygen_value"
                    android:layout_marginLeft="40dp"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    />
                
	      <TextView android:text="Dissolved Oxygen"
	                android:textSize="20dp"
	                android:textColor="#ffffff"
	                android:layout_below="@id/dissolved_oxygen_image"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginLeft="10dp"	          
	          /> 
        

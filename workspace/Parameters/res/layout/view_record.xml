    <RelativeLayout android:id="@+id/colorimeter"
          android:layout_toRightOf="@id/chloride"
          android:layout_below="@id/biochemical"
          android:layout_width="175dp"
          android:layout_height="150dp"
          android:background="#1E90FF" 
          android:layout_marginLeft="10dp"
          android:layout_marginRight="10dp"
          android:layout_marginTop="10dp" >
        
		        <ImageView android:id="@+id/colorimeter_image"
			                 android:layout_width="75dp"
			                 android:layout_height="125dp"
			                 android:layout_marginLeft="10dp"
			                 android:layout_marginTop="0dp"
			                 android:src="@drawable/image_5"
			                    />  
			                    
		        <TextView android:id="@+id/colorimeter_value"
	                android:layout_toRightOf="@id/colorimeter_image"
	                android:layout_marginLeft="40dp"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:textColor="#ffffff"
	                android:textSize="25dp"
	                android:text=""
	          />

		        <ImageView 
		            android:id="@+id/colorimeter_arrow"
		            android:layout_toRightOf="@id/colorimeter_image"
		            android:layout_below="@id/colorimeter_value"
		            android:layout_marginLeft="40dp"
		            android:layout_width="25dp"
		            android:layout_height="25dp"
		            />
		        	      
	      <TextView android:text="colorimeter"
	                android:textSize="20dp"
	                android:textColor="#ffffff"
	                android:layout_below="@id/colorimeter_image"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginLeft="10dp"	          
	          /> 
      </RelativeLayout>
      
    
    <RelativeLayout android:id="@+id/conductivity"
          android:layout_width="175dp"
          android:layout_height="150dp"
          android:background="#1E90FF"
          android:layout_marginLeft="12.5dp"
          android:layout_marginTop="10dp"
          android:layout_below="@id/chloride" >
		        
		        <ImageView android:id="@+id/conductivity_image"
			                 android:layout_width="75dp"
			                 android:layout_height="125dp"
			                 android:layout_marginLeft="10dp"
			                 android:layout_marginTop="0dp"
			                 android:src="@drawable/image_6"
			                    />  
                <TextView android:id="@+id/conductivity_value"
	                android:layout_toRightOf="@id/conductivity_image"
	                android:layout_marginLeft="40dp"
	                android:layout_width="wrap_content"

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">
	 <LinearLayout
	     android:layout_width="fill_parent"
	     android:layout_height="35dp"
	     android:orientation="horizontal"
	     android:background="@color/lightblue" >
	     <TextView 
	         android:id="@+id/date_text"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_marginLeft="5dp"
	         android:textColor="#ffffff"
	         android:textSize="25dp"
	         />
	     <ImageView 
	         android:layout_width="wrap_content"
	         android:layout_height="30dp"
	         android:layout_marginLeft="200dp"
	         android:layout_gravity="center"
	         android:src="@drawable/android_calender"
	         android:onClick="calendar"
	         />
	     
	 </LinearLayout>
<ScrollView 
android:id="@+id/ScrollView01" android:layout_width="fill_parent"
android:layout_height="fill_parent" >
<RelativeLayout 
    android:id="@+id/parameter"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
   
    
       <RelativeLayout android:id="@+id/ammonia"
          android:layout_width="175dp"
          android:layout_height="150dp"
          android:background="#1E90FF"
          android:layout_marginLeft="12.5dp"
          android:layout_marginTop="25dp"
           >
	      <ImageView android:id="@+id/ammonia_image"
	                 android:layout_width="75dp"
	                 android:layout_height="125dp"
	                 android:layout_marginLeft="10dp"
	                 android:layout_marginTop="0dp"
	                 android:src="@drawable/image_2"
	                    />  
	      <TextView android:id="@+id/ammonia_value"
	                android:layout_toRightOf="@id/ammonia_image"
	                android:layout_marginLeft="40dp"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:textColor="#ffffff"
	                android:textSize="25dp"
	                
	          />
	      <ImageView 
	          android:id="@+id/ammonia_arrow"
	          android:layout_toRightOf="@id/ammonia_image"
	          android:layout_below="@id/ammonia_value"
	          android:layout_width="25dp"
	          android:layout_height="25dp"
	          android:layout_marginLeft="40dp"
	          />
	      
	      <TextView android:text="Ammonia"
	                android:textSize="20dp"
	                android:textColor="#ffffff"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_below="@id/ammonia_image"
	                android:layout_marginLeft="10dp"	          
	          />
	      
 </RelativeLayout>
       
    <RelativeLayout android:id="@+id/biochemical"
          android:layout_toRightOf="@id/ammonia"
          android:layout_width="175dp"
          android:layout_height="150dp"
          android:background="#1E90FF"
          android:layout_marginLeft="10dp"
          android:layout_marginRight="10dp"
          android:layout_marginTop="25dp" >
         <ImageView android:id="@+id/biochemical_image"
	                 android:layout_width="75dp"
	                 android:layout_height="125dp"
	                 android:layout_marginLeft="10dp"
	                 android:layout_marginTop="0dp"
	                 android:src="@drawable/image_3"
	                    /> 
	    <TextView android:id="@+id/biochemical_value"
	                android:layout_toRightOf="@id/biochemical_image"
	                android:layout_marginLeft="40dp"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:textColor="#ffffff"
	                android:textSize="25dp"
	                android:text=""
	          />
	      
	    <ImageView 
	        android:id="@+id/biochemical_arrow"
	        android:layout_toRightOf="@id/biochemical_image"
	        android:layout_below="@id/biochemical_value"
	        android:layout_marginLeft="40dp"
	        android:layout_width="25dp"
	        android:layout_height="25dp"
	        />
	    
	      <TextView android:text="Biochemical"
	                android:textSize="20dp"
	                android:textColor="#ffffff"
	                android:layout_below="@id/biochemical_image"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginLeft="10dp"	          
	          />                
        
</RelativeLayout>
    
 <RelativeLayout android:id="@+id/chloride"

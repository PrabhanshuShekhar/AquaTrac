	                android:textSize="25dp"
	                android:text=""
	          />
	      
				<ImageView  
				    android:id="@+id/free_chlorine_arrow"
				    android:layout_toRightOf="@id/free_chlorine_image"
				    android:layout_below="@id/free_chlorine_value"

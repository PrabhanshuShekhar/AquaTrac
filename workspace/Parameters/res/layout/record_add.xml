         <ImageView android:id="@+id/biochemical_image"
	                 android:layout_width="40dp"
	                 android:layout_height="75dp"
	                 android:layout_marginLeft="10dp"
	                 android:layout_marginTop="5dp"
	                 android:src="@drawable/image_3"
	                    /> 
	    <TextView android:id="@+id/biochemical_value"
	                android:layout_toRightOf="@id/biochemical_image"
	                android:layout_marginLeft="50dp"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:textColor="#ffffff"
	                android:textSize="15dp"
	                android:text="44.45"
	          />
	      
	      <TextView android:text="Biochemical"
	                android:textSize="12dp"
	                android:textColor="#ffffff"
	                android:layout_below="@id/biochemical_image"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginLeft="10dp"	          
	          />                
        
</RelativeLayout>
    
 <RelativeLayout android:id="@+id/chloride"
          android:layout_width="145dp"
          android:layout_height="100dp"
          android:background="#1E90FF"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="10dp"
          android:onClick="clicker"
          android:layout_below="@id/ammonia" >
         
        <ImageView android:id="@+id/chloride_image"
	                 android:layout_width="40dp"
	                 android:layout_height="75dp"
	                 android:layout_marginLeft="10dp"
	                 android:layout_marginTop="5dp"
	                 android:src="@drawable/image_4"
	                    /> 
	    <TextView android:id="@+id/chloride_value"
	                android:layout_toRightOf="@id/chloride_image"

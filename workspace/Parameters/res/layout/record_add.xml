<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/ScrollView01" android:layout_width="fill_parent"
android:layout_height="fill_parent" >
<RelativeLayout 
    android:id="@+id/parameter"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
   
    
       <RelativeLayout android:id="@+id/ammonia"
          android:layout_width="145dp"
          android:layout_height="100dp"
          android:background="#1E90FF"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="25dp"
          android:onClick="clicker"
           >
	      <ImageView android:id="@+id/ammonia_image"
	                 android:layout_width="40dp"
	                 android:layout_height="75dp"
	                 android:layout_marginLeft="10dp"
	                 android:layout_marginTop="5dp"
	                 android:src="@drawable/image_2"
	                    />  
	      <TextView android:id="@+id/ammonia_value"
	                android:layout_toRightOf="@id/ammonia_image"
	                android:layout_marginLeft="50dp"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:textColor="#ffffff"
	                android:textSize="15dp"
	                android:text="34.45"
	          />
	      
	      <TextView android:text="Ammonia"
	                android:textSize="12dp"
	                android:textColor="#ffffff"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_below="@id/ammonia_image"
	                android:layout_marginLeft="10dp"	          
	          />
	      
 </RelativeLayout>
       
    <RelativeLayout android:id="@+id/biochemical"
